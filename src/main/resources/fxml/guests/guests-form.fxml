<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookings.controllers.guests.FormController">
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <TextField fx:id="nameTF" promptText="name" />
            <TextField fx:id="phoneTF" promptText="phone" />
            <TextField fx:id="emailTF" promptText="email" />

            <CheckBox fx:id="isGroupCB" mnemonicParsing="false" text="is Group" />


            <Button mnemonicParsing="false" onAction="#onSubmitClicked" text="Submit" />
        </VBox>
    </center>
</BorderPane>
